{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "block-spacing": "error",
        "semi": "error",
        "quotes": ["error", "single"],
        "indent": ["error", 2],
        "eqeqeq": "error",
        "curly":"error", 
        "no-var": "error",
        "no-unused-vars": "warn"
    }
}
